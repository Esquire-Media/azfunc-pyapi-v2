{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    },
    "logLevel": {
      "azure": "Warning"
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.*, 5.0.0)"
  },
  "extensions": {
    "durableTask": {
      "hubName": "%TASK_HUB_NAME%",
      "maxConcurrentActivityFunctions": 5,
      "maxConcurrentOrchestratorFunctions": 2
    }
  },
  "functionTimeout": "00:10:00"
}